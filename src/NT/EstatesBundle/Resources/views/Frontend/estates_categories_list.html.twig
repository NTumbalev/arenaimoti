        <div class="mainBody">
            <div class="wrapper">
                {% set haveSideBar = false %}
                {% if sideBar is defined and sideBar is not null and sideBar|length %}
                    {% set haveSideBar = true %}
                {% endif %}
                <div class="mainContent{% if haveSideBar == false %} full{% endif %}">
                    <h1 class="titleMain">{{ content.title }}</h1>

                    {% if content.description is defined and content.description is not null and content.description|length %}
                        <article class="article">
                            {{ content.description|raw }}
                        </article>
                    {% endif %}

                    <div class="boxes boxesEstates">
                        {% for estateCategory in estatesCategories %}
                            <div class="box">
                        <a href="{{ path(estateCategory.route, estateCategory.routeParams()) }}" class="photo" style="background-image: url({% if estateCategory.translations[app.request.locale].image is defined and estateCategory.translations[app.request.locale].image is not null and estateCategory.translations[app.request.locale].image|length %}{% path estateCategory.translations[app.request.locale].image, 'index' %}{% else %}'/assets//images/no-img.jpg'{% endif %});"></a>

                                <div class="content">
                                    <p class="title">
                                        <a href="{{ path(estateCategory.route, estateCategory.routeParams()) }}">
                                            {{ estateCategory.title }}
                                        </a>
                                    </p>

                                    {% if estateCategory.simpleDescription is defined and estateCategory.simpleDescription is not null and estateCategory.simpleDescription|length %}
                                        <div class="text">
                                            <p>{{ estateCategory.simpleDescription }}</p>
                                        </div>
                                    {% endif %}
                                </div>
                            </div><!-- /.box -->
                        {% endfor %}
                    </div><!-- /.boxes -->

                    {% if estatesCategories is defined and estatesCategories|length %}
                        {{ pager.pager(estatesCategories, 'estates_categories_list') }}
                    {% endif %}
                </div><!-- /.mainContent -->

                {% if haveSideBar %}
                    <aside class="sidebar">
                        <p class="titleAside">{{ content.title }}</p>

                        <nav class="navAside">
                            {{ sideBar|raw }}
                        </nav><!-- /.navAside -->
                    </aside><!-- /.sidebar -->
                {% endif %}
            </div><!-- /.wrapper -->
        </div><!-- /.mainBody -->
    </section>
    #}
